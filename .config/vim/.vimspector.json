{
    "configurations": {
        "CPP: Run current script": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "justMyCode#json": "${justMyCode:true}",
                "program": "cmake-build-Debug/status_sender_unittests",
                "request": "launch",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "ignoreFailures": true,
                        "text": "-enable-pretty-printing"
                    }
                ]
            }
        }
    }
}
